  </link>
  <joint name="gripper_side_joint" type="fixed">
    <parent link="ur_to_robotiq_link"/>
    <child link="gripper_mount_link"/>
    <!-- <origin xyz="0 0 0.011" rpy="0 ${-pi/2} ${pi/2}"/> -->
    <origin rpy="0 0 0" xyz="0 0 0.011"/>
  </joint>
